{
  "name": "thin-component",
  "version": "3.1.3",
  "description": "lazy table for react component",
  "keywords": [
    "ant",
    "design",
    "react",
    "react-component",
    "component",
    "components",
    "ui",
    "lazy table"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyValley/thin-component.git"
  },
  "private": false,
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env PROXY_ENV=dev webpack-dev-server --hot --config ./scripts/webpack.dev.config.js",
    "stylelint": "stylelint 'src/**/*.less'",
    "stylelint:fix": "stylelint 'src/**/*.less' --fix",
    "eslint": "eslint 'src/**/*.{js,jsx}'",
    "eslint:fix": "eslint 'src/**/*.{js,jsx}' --fix -o airbnb.html --format html",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "prettier": "npm run prettier:js && npm run prettier:style",
    "prettier:js": "prettier --config ./.prettierrc --write 'src/**/*.js'",
    "prettier:style": "prettier --config ./.prettierrc --write 'src/**/*.less'",
    "less": "lessc ./src/lazy-table/index.less ./src/lazy-table/index.css"
  },
  "author": "easyvalley",
  "babel": {
    "presets": [
      "./.babelrc.js"
    ]
  },
  "module": "src/index.js",
  "devDependencies": {
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.2.6",
    "babel-loader": "~7.1.2",
    "babel-plugin-import": "~1.6.3",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-preset-env": "~1.6.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "chalk": "^2.4.2",
    "co": "^4.6.0",
    "conventional-changelog-cli": "^1.3.22",
    "cos-nodejs-sdk-v5": "^2.5.9",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-qtrade": "^1.0.4",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "~1.7.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "less": "^3.9.0",
    "less-loader": "~4.1.0",
    "lint-staged": "^7.0.5",
    "prettier": "^1.18.2",
    "rimraf": "~2.6.2",
    "style-loader": "~0.19.1",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^3.0.1",
    "stylelint-prettier": "^1.1.1",
    "url-loader": "~0.6.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cos-plugin": "^1.1.2",
    "webpack-dev-server": "^2.11.5",
    "webpack-merge": "~4.1.1",
    "webpack-notifier": "~1.6.0"
  },
  "dependencies": {
    "antd": "^3.21.3",
    "babel-polyfill": "~6.26.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "latest",
    "qs": "latest",
    "querystring": "^0.2.0",
    "react": "~16.6.0",
    "react-dom": "~16.6.0",
    "react-resizable": "^1.10.1",
    "react-router-dom": "~4.3.1",
    "webpack": "~3.10.0"
  }
}
